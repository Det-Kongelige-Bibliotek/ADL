    <field name="text_tesim">Altid fremad </field>
    <field name="text_tesim">Floden flyder, </field>
    <field name="text_tesim">saadan lider </field>
    <field name="text_tesim">Livets Løb. </field>
    <field name="text_tesim">Stund, som svandt </field>
    <field name="text_tesim">for stedse svandt, </field>
    <field name="text_tesim">og til sidst </field>
    <field name="text_tesim">er Døden sikker </field>
  </doc>
