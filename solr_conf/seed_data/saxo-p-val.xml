    <field name="genre_ssi">poetry</field>
    <field name="text_tesim">Gammel mand </field>
    <field name="text_tesim">har liden Gammen, </field>
    <field name="text_tesim">dumme Øjne, </field>
    <field name="text_tesim">døve Øren, </field>
    <field name="text_tesim">ælde lammer </field>
    <field name="text_tesim">alle Lemmer, </field>
    <field name="text_tesim">sløver Vid </field>
    <field name="text_tesim">og slapper Viljen. </field>
  </doc>
  <doc>
    <field name="type_ssi">leaf</field>
