    <field name="text_tesim">du siger </field>
    <field name="text_tesim">jeg sagde </field>
    <field name="text_tesim">du spørger </field>
    <field name="text_tesim">jeg svarer </field>
    <field name="text_tesim">jeg drømte </field>
    <field name="text_tesim">om natten </field>
    <field name="text_tesim">langt nede </field>
    <field name="text_tesim">langt borte </field>
    <field name="text_tesim">fra dig. - </field>
    <field name="text_tesim">i sengen </field>
    <field name="text_tesim">jeg drømte </field>
    <field name="text_tesim">jeg troede </field>
    <field name="text_tesim">vi sammen </field>
    <field name="text_tesim">om natten </field>
    <field name="text_tesim">i pyjamas </field>
    <field name="text_tesim">var jeg. - </field>
    <field name="text_tesim">[i] ingenting? </field>
    <field name="text_tesim">var du. </field>
    <field name="text_tesim">du var eet </field>
    <field name="text_tesim">med dig sæl </field>
    <field name="text_tesim">og med mig, - </field>
    <field name="text_tesim">du luktede </field>
    <field name="text_tesim">himmelsk </field>
    <field name="text_tesim">af aande </field>
    <field name="text_tesim">og sved. </field>
    <field name="text_tesim">du bed mig </field>
    <field name="text_tesim">paa næsen </field>
    <field name="text_tesim">men dejligt </field>
    <field name="text_tesim">det lød. </field>
    <field name="text_tesim">som paa skyer </field>
    <field name="text_tesim">vi laa jo </field>
    <field name="text_tesim">vi var kun </field>
    <field name="text_tesim">hvad andet </field>
    <field name="text_tesim">men sammen, </field>
    <field name="text_tesim">vi svømmede </field>
    <field name="text_tesim">rundt i </field>
    <field name="text_tesim">hinanden. </field>
    <field name="text_tesim">saa faldt vi </field>
    <field name="text_tesim">faldt hurtig </field>
    <field name="text_tesim">vi to. </field>
    <field name="text_tesim">i vanvidet op. </field>
    <field name="text_tesim">saa steg du </field>
    <field name="text_tesim">saa faldt jeg </field>
    <field name="text_tesim">fra himlen </field>
    <field name="text_tesim">og endte </field>
    <field name="text_tesim">i sengen </field>
    <field name="text_tesim">paa jorden </field>
    <field name="text_tesim">langt borte fra dig </field>
